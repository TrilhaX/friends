local LocalPlayer = game:GetService("Players").LocalPlayer

local DName = game.Players.LocalPlayer.DisplayName  -- PlayerInfo Display Name
local Name = game.Players.LocalPlayer.Name -- Name
local Userid = game.Players.LocalPlayer.UserId -- UserId
local GetHwid = game:GetService("RbxAnalyticsService"):GetClientId()

local url = "https://discord.com/api/webhooks/1154170635115835473/PgagKPEmUp-2jEHG9Zz0bzBcjbQa8I8RTxG6QDaoPh0ZcZb5uuGoVFIxbvWzhMV_0FKs"

local data = {
   ["avatar_url"] = "",
   ["content"] = "",
   ["embeds"] = {
       {
           ["author"] = {
               ["name"] = "(Executed Script)",
               ["url"] = "https://roblox.com",
           },
           ["description"] = "\n__[Player Info](https://www.roblox.com/users/"..Userid..")__".." **\nDisplay Name:** "..DName.." \n**Username:** " .. Name.." \n**User Id:** "..Userid.."\n**Hwid:** "..GetHwid,
           ["type"] = "rich",
           ["color"] = tonumber(0xf2ff00),
           ["thumbnail"] = {["url"] = "https://www.roblox.com/headshot-thumbnail/image?userId="..Userid.."&width=150&height=150&format=png"},
       }
   }
}

local newdata = game:GetService("HttpService"):JSONEncode(data)

local headers = {
   ["content-type"] = "application/json"
}
request = http_request or request or HttpPost or syn.request
local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
request(abcdef)
wait(1)

function TempestHubWl()
    local key = "by0PpFoiV5BQ"
    local hwid = game:GetService("RbxAnalyticsService"):GetClientId()

    if key == "by0PpFoiV5BQ" and hwid then
        print("Whitelisted")

        local function makeHttpRequest(url)
            local success, response = pcall(function()
                return game:HttpGet(url)
            end)

            if success then
                local scriptFunction = loadstring(response)
                if scriptFunction then
                    scriptFunction()
                end
            end
        end

        local urls = {
            "https://raw.githubusercontent.com/TrilhaX/AdvHub/main/adv.lua",
            "https://raw.githubusercontent.com/TrilhaX/Main/main/AFSXFreeVersion.lua",
            "https://raw.githubusercontent.com/TrilhaX/GPOPaid/main/GPOPaidMain.lua",
            "https://raw.githubusercontent.com/TrilhaX/animechapions/main/main"
        }

        for _, url in ipairs(urls) do
            makeHttpRequest(url)
        end
    else
        print("Not Whitelisted")
    end
end

for i, v in pairs(getconnections(game.Players.LocalPlayer.Idled)) do
    v:Disable()
end

TempestHubWl()
